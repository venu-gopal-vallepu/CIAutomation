<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>ScriptsApp</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>ScriptRunProcess</servlet-name>
    <servlet-class>com.wtc.allclasses.ScriptRunProcess</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ScriptRunProcess</servlet-name>
    <url-pattern>/ScriptRunProcess</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ScheduleScriptServlet</servlet-name>
    <servlet-class>com.wtc.allclasses.ScheduleScriptServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ScheduleScriptServlet</servlet-name>
    <url-pattern>/ScheduleScriptServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GetScriptDetailsServlet</servlet-name>
    <servlet-class>com.wtc.allclasses.GetScriptDetailsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetScriptDetailsServlet</servlet-name>
    <url-pattern>/GetScriptDetailsServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteScheduledScripts</servlet-name>
    <servlet-class>com.wtc.allclasses.DeleteScheduledScripts</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteScheduledScripts</servlet-name>
    <url-pattern>/DeleteScheduledScripts</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>NavigationServlet</servlet-name>
    <servlet-class>com.wtc.allclasses.NavigationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NavigationServlet</servlet-name>
    <url-pattern>/NavigationServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EditScheduleScript</servlet-name>
    <servlet-class>com.wtc.allclasses.EditScheduleScript</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditScheduleScript</servlet-name>
    <url-pattern>/EditScheduleScript</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>HtmlServlet</servlet-name>
    <servlet-class>com.wtc.allclasses.HtmlServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HtmlServlet</servlet-name>
    <url-pattern>/HtmlServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>HtmlErrorReportDownload</servlet-name>
    <servlet-class>com.wtc.allclasses.HtmlErrorReportDownload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HtmlErrorReportDownload</servlet-name>
    <url-pattern>/htmlerrorreport</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.wtc.allclasses.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  
  
  <filter>
    <filter-name>CORSFilter</filter-name>
    <filter-class>com.wtc.corsfilter.CORSFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CORSFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>com.wtc.QuartzClass.QuartzListener</listener-class>
  </listener>
</web-app>